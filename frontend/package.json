{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "npm run codegen",
		"dev": "npm run codegen && vite dev",
		"build": "npm run codegen && vite build",
		"codegen": "bash scripts/codegen.sh",
		"preview": "vite preview",
		"test": "npm run test:integration:once && npm run test:unit:once",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "PWTEST_WATCH=1 playwright test",
		"test:integration:once": "playwright test",
		"test:integration:ui": "playwright test --ui",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"up-deps": "npx npm-check-updates -i"
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.2.8",
		"@iconify-json/heroicons": "^1.2.2",
		"@iconify-json/lucide": "^1.2.39",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.1.4",
		"@types/d3": "^7.4.3",
		"@types/eslint": "^9.6.1",
		"@types/lodash": "^4.17.16",
		"@types/node": "^22.14.1",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.6",
		"sveltekit-search-params": "^4.0.0-next.0",
		"tailwindcss": "^4.1.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.31.0",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.3.3",
		"vitest": "^3.1.2"
	},
	"type": "module",
	"dependencies": {
		"@creditkarma/thrift-server-core": "^1.0.4",
		"@creditkarma/thrift-typescript": "^3.7.6",
		"@layerstack/svelte-actions": "^1.0.0",
		"@layerstack/svelte-table": "^1.0.1-next.0",
		"@layerstack/tailwind": "^2.0.0-next.2",
		"@layerstack/utils": "^1.0.0",
		"@tailwindcss/vite": "^4.1.4",
		"d3": "^7.9.0",
		"layerchart": "https://pkg.pr.new/layerchart@67759e2",
		"lodash": "^4.17.21",
		"runed": "^0.25.0",
		"svelte-inspect-value": "0.6.2",
		"svelte-ux": "^2.0.0-next.4"
	},
	"overrides": {
		"cross-spawn": "^7.0.6",
		"cookie": "^0.7.0",
		"glob": "^9.0.0",
		"vite": "^6.3.3",
		"nanoid": "^3.3.8"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.40.0"
	}
}
