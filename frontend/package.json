{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "npm run codegen",
		"dev": "npm run codegen && vite dev",
		"build": "npm run codegen && vite build",
		"codegen": "bash scripts/codegen.sh",
		"preview": "vite preview",
		"test": "npm run test:integration:once && npm run test:unit:once",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "PWTEST_WATCH=1 playwright test",
		"test:integration:once": "playwright test",
		"test:integration:ui": "playwright test --ui",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"up-deps": "npx npm-check-updates -i"
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.2.8",
		"@iconify-json/heroicons": "^1.2.2",
		"@iconify-json/lucide": "^1.2.34",
		"@internationalized/date": "^3.7.0",
		"@playwright/test": "^1.50.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.1.0",
		"@types/d3": "^7.4.3",
		"@types/eslint": "^9.6.1",
		"@types/lodash": "^4.17.16",
		"@types/node": "^22.13.17",
		"bits-ui": "^0.21.16",
		"cmdk-sv": "^0.0.18",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.5.0",
		"globals": "^15.15.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.6",
		"svelte-check": "^4.1.5",
		"svelte-intersection-observer": "^1.0.0",
		"svelte-radix": "^1.1.1",
		"sveltekit-search-params": "^4.0.0-next.0",
		"tailwindcss": "^4.1.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.29.0",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.2.4",
		"vitest": "^3.1.1"
	},
	"type": "module",
	"dependencies": {
		"@creditkarma/thrift-server-core": "^1.0.4",
		"@creditkarma/thrift-typescript": "^3.7.6",
		"@layerstack/svelte-actions": "^1.0.0",
		"@layerstack/svelte-table": "^1.0.1-next.0",
		"@layerstack/tailwind": "^2.0.0-next.2",
		"@layerstack/utils": "^1.0.0",
		"@tailwindcss/vite": "^4.1.0",
		"d3": "^7.9.0",
		"dotenv": "^16.4.7",
		"layerchart": "https://pkg.pr.new/layerchart@67759e2",
		"lodash": "^4.17.21",
		"runed": "^0.25.0",
		"svelte-inspect-value": "0.3.0",
		"svelte-ux": "^2.0.0-next.4",
		"tailwind-variants": "^0.3.1"
	},
	"overrides": {
		"cross-spawn": "^7.0.6",
		"cookie": "^0.7.0",
		"glob": "^9.0.0",
		"vite": "^6.2.4",
		"nanoid": "^3.3.8"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.34.8"
	}
}
