<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%date] {%logger{0}} %level - %message%n</pattern>
        </encoder>
    </appender>
    <logger name="ai.chronon" level="INFO" />

    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>